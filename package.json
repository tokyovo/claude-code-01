{
  "name": "personal-finance-tracker-api",
  "version": "1.0.0",
  "description": "Backend API for Personal Finance Tracker application",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:dev": "ts-node src/server.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:ci": "NODE_ENV=test jest --coverage --watchAll=false --passWithNoTests",
    "test:unit": "NODE_ENV=test jest --testPathPattern=tests/unit",
    "test:integration": "NODE_ENV=test jest --testPathPattern=tests/integration",
    "test:api": "NODE_ENV=test jest --testPathPattern=tests/integration",
    "test:debug": "NODE_ENV=test node --inspect-brk node_modules/.bin/jest --runInBand",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "db:up": "docker-compose up -d postgres",
    "db:down": "docker-compose stop postgres",
    "db:logs": "docker-compose logs -f postgres",
    "db:reset": "npm run migrate:rollback:all && npm run migrate:latest && npm run seed:run",
    "redis:up": "docker-compose up -d redis",
    "redis:down": "docker-compose stop redis",
    "redis:logs": "docker-compose logs -f redis",
    "redis:flush": "docker-compose exec redis redis-cli FLUSHALL",
    "services:up": "docker-compose up -d postgres redis",
    "services:down": "docker-compose stop postgres redis",
    "services:status": "docker-compose ps",
    "dev:full": "npm run services:up && npm run dev",
    "dev:services": "docker-compose --profile dev up -d",
    "dev:clean": "npm run docker:clean && npm run dev:services",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:rollback:all": "knex migrate:rollback --all",
    "migrate:status": "knex migrate:status",
    "migrate:make": "knex migrate:make",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down",
    "seed:run": "knex seed:run",
    "seed:make": "knex seed:make",
    "db:setup": "npm run migrate:latest && npm run seed:run",
    "db:fresh": "npm run docker:clean && npm run db:up && sleep 5 && npm run db:setup",
    "migrate:test": "NODE_ENV=test knex migrate:latest",
    "migrate:test:rollback": "NODE_ENV=test knex migrate:rollback",
    "seed:test": "NODE_ENV=test knex seed:run",
    "db:test:setup": "NODE_ENV=test npm run migrate:test && npm run seed:test",
    "db:test:reset": "NODE_ENV=test npm run migrate:test:rollback && npm run migrate:test && npm run seed:test"
  },
  "keywords": [
    "finance",
    "api",
    "express",
    "typescript",
    "personal-finance"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/knex": "^0.15.2",
    "@types/pg": "^8.15.5",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^7.2.0",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "joi": "^17.12.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "rate-limit-redis": "^4.2.2",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.1",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "jest": "^30.1.1",
    "jest-environment-node": "^30.1.1",
    "nodemon": "^3.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
